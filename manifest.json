{
  "manifest_version": 2,
  "name": "Tipdoma",
  "version": "1.0",
  "icons": {
    "128": "favicon.ico"
  },
  "content_scripts": [
    {
      "matches": [ "*://*/*" ],
      "js": ["jquery-3.2.1.min.js", "content.js"]
    }
  ],
  "background": {
    "scripts": ["jquery-3.2.1.min.js", "background.js"]
  },
  "permissions": [
    "http://tipdoma.ru/*",
    "activeTab"
  ],
  "browser_action": {
    "default_title": "Tipdoma",
    "default_icon": "favicon.ico",
    "default_popup": "index.html"
  }
}