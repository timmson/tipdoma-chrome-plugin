{
  "manifest_version": 2,
  "name": "Tipdoma",
  "version": "1.0",
  "icons": {
    "256": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [ "*://*.cian.ru/sale/flat/*" ],
      "js": ["jquery-3.2.1.min.js", "content.js"],
      "css": ["content.css"]
    }
  ],
  "background": {
    "scripts": ["jquery-3.2.1.min.js", "background.js"]
  },
  "permissions": [
    "http://tipdoma.ru/*",
    "activeTab"
  ],
  "browser_action": {
    "default_title": "Tipdoma",
    "default_icon": "icon.png"
  }
}